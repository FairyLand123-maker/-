.searchTop{
  height: 90rpx;background: #f5f5f5;
  display: flex;
  justify-content: center;
  align-items: center;
}
.searchTopBox{
  background: #fff;
  height: 60rpx;
  width: 710rpx;
  border-radius: 60rpx;
  display: flex;
  align-items: center;
  line-height: 60rpx;
}
.searchTopBox image{
  width: 35rpx;
  height: 35rpx;
  margin:0 15rpx 0 40rpx;
}
.searchTitle{
  height: 80rpx;
  margin:0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}
.searchTitle image{
  width: 52rpx;
  height: 52rpx
}
.history{clear: both;}
.historyList{margin:10rpx 20rpx 0;}
.historyItem{
  height: 60rpx;
  padding:0 20rpx;
  line-height: 60rpx;
  background: #f5f5f5;
  float:left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  font-size: 24rpx;
  color: #999;
  border-radius: 60rpx;
}