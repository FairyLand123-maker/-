
 .note{
  width: 96%;
  margin: auto;
  border-radius: 20rpx;
 }
.noteItem{
  height: 300rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  border: 2rpx solid #cccccc;
}
.list_one{
  height: 100rpx;
  display: flex;
  justify-items: center;
  align-items: center;
  text-align: center;
  position: relative;
}
.list_one .userInfo{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  border: 2rpx solid gray;
  margin-right: 42rpx;
  margin-top: 17rpx;
  margin-left: 15rpx;
  text-align: center;
  line-height: 80rpx;
  color: #de7d2c;
}
.list_one .userName{
  font-size: 30rpx;
    color: gray;
}
.list_one .number{
  font-size: 30rpx;
    color: gray;
    margin-right: 20rpx;
    position: absolute;
    left: 280rpx;
}
.list_one .price{
  font-size: 30rpx;
    color: gray;
    float: right;
    position: absolute;
    left: 460rpx;
}
.list_one .points{
  font-size: 30rpx;
  color: gray;
  position: absolute;
  left: 552rpx;
}

.list_two{
  height:200rpx;
  display: flex;
  justify-items: center;
  align-items: center;

}
.list_two .left{
  width: 65%;
    height: 95%;
    margin: 5rpx 0 0 10rpx;
}
.list_two .left .title{
  font-size: 33rpx;
  height: 70rpx;
  margin-top: 20rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
}
.list_two .left .summer{
  font-size: 35rpx;
  height: 100rpx;
  height: 100rpx;
  line-height: 50rpx;
  line-height: 50rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
}
.list_two .right{
  flex: 1;
  width: 100rpx;
  height: 150rpx;
  margin-right: 5rpx;
  margin-left: 5rpx;
}
.list_two .right image{
  width: 100%;
  height: 100%;
}
